; This is CPC System 42 tracks format spec
[cpcsys42]
Description = CPC System 42 tracks
Sidesness = Alt
Cylinders = 42
Heads = 1
Sectors = 9
SecBase = 65
SecSize = 512
DataRate = SD
RWGap = 42
FmtGap = 82
